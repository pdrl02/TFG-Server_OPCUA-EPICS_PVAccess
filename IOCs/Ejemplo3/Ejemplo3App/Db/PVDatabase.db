# Valor que se recibe de una lectura.
record(int64in, "ejemplo3:int64in") {
    field(VAL, "100")
    field(DESC, "64-bit Integer Input")
    field(SCAN, "1 second")
    field(EGU, "Counter")
    field(LOPR, "-2000")
    field(HOPR, "100000")
}

# Valor que sería enviado a un proceso, campo, hardware...
record(int64out, "ejemplo3:int64out") {
    field(VAL, "2000")
    field(DESC, "64-bit Integer Output")
    field(OMSL, "supervisory")              # The new output value is taken from the VAL field, which may have been set from elsewhere.
    field(EGU, "J")
    field(DRVL, "1000")
    field(DRVH, "5000")
    #LOPR
    #HOPR
}

