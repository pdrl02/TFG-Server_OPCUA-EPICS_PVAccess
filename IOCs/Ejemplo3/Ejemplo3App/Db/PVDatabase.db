# Valor que se recibe de una lectura.
record(mbbi, "ejemplo3:mbbi") {
    field(DTYP, "Soft Channel")
    field(ZRVL, "0")  # Valor para el estado 0
    field(ONVL, "1")  # Valor para el estado 1
    field(TWVL, "2")  # Valor para el estado 2
    field(THVL, "3")  # Valor para el estado 3
    field(ZRST, "State 0")  # Nombre del estado 0
    field(ONST, "State 1")  # Nombre del estado 1
    field(TWST, "State 2")  # Nombre del estado 2
    field(THST, "State 3")  # Nombre del estado 3
    field(VAL, "0")
    field(DESC, "Multi-Bit Binary Input with 4 states")
}

